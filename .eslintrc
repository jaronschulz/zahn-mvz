{
  "parser": "babel-eslint",

  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "prettier/react",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "strict": 0,
    "prettier/prettier": "error",
    "react/prop-types": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
